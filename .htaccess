RewriteEngine on
RewriteOptions inherit
Options +FollowSymLinks
AddDefaultCharset utf-8
<ifmodule mod_php4.c>
	php_value zlib.output_compression 16386
</ifmodule>
AddType application/x-httpd-php .php .inc .html .htm
DirectoryIndex index.php index.html index.htm
<IfModule mod_gzip.c>
	mod_gzip_on Yes
	mod_gzip_dechunk Yes
	mod_gzip_item_include file \.(html?|txt|css|js|php)$
	mod_gzip_item_include mime ^application/x-javascript.*
	mod_gzip_item_exclude mime ^asset/.*
	mod_gzip_item_exclude rspheader ^Content-Encoding:.*gzip.*
</IfModule>
RewriteBase /

RewriteRule ^fia$ fia/ [NC,QSA,END]
RewriteRule ^fia\/$ index.php?oredirect=index [NC,QSA,END]
RewriteRule ^fia\/(.*)$ index.php?oredirect=$1 [NC,QSA,END]

RewriteRule ^favicon\.ico$ content/asset/favicon.ico [NC,QSA,END]

RewriteRule ^asset\/$ content/asset/ [NC,QSA,END]
RewriteRule ^asset\/(.*)$ content/asset/$1 [NC,QSA,END]

RewriteRule ^content\/asset$ content/asset/ [NC,QSA,END]
RewriteRule ^content\/piece$ content/piece/ [NC,QSA,END]
RewriteRule ^content\/view$ content/view/ [NC,QSA,END]

RewriteRule ^content\/$ index.php?oredirect=index [NC,QSA,END]
RewriteRule ^content\/(.*)$ index.php?oredirect=$1 [NC,QSA,END]

RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f

Options -Indexes

#RewriteRule ^(.*)$ $1.php [NC,QSA,END]
RewriteRule ^((?!asset\/.*$)(?!content\/.*$).*)$ /index.php?olink=$1 [NC,QSA,END]
ErrorDocument 403 /index.php?olink=E403
ErrorDocument 404 /index.php?olink=E404